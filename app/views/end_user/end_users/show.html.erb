<div class="container">
  <div class="row">
    <div class="col-md">
      <h2 class= "mt-3">マイページ</h2>
      <table class= "table table-hover">
        <tr>
          <th>氏名</th>
          <td><%= @end_user.first_name %><%= @end_user.last_name %></td>
        </tr>
        <tr>
          <th>カナ</th>
          <td><%= @end_user.kana_first_name %><%= @end_user.kana_last_name %></td>
        </tr>
         <tr>
          <th>郵便番号</th>
          <td><%= @end_user.postal_code %></td>

        </tr>
         <tr>
          <th>住所</th>
          <td><%= @end_user.address %></td>
        </tr>
        <tr>
          <th>電話番号</th>
          <td><%= @end_user.phone_number %></td>
        </tr>
        <tr>
          <th>メールアドレス</th>
          <td><%= @end_user.email %></td>
        </tr>
      </table>
      <%= link_to "編集する", edit_end_user_path(current_end_user), class: "btn btn-primary" %>
    </div>
  </div>
</div>