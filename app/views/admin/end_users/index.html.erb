<div class="container">
  <div class="row">
    <div class="col-md">
      <h3>会員一覧</h3>
      <table class= "table">
        <tr>
          <th>会員id</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>会員ステータス</th>
        </tr>
        <% @end_users.each do |end_user| %>
        <tr>
            <td><%= end_user.id %></td>
            <td>
              <%= link_to admin_end_user_path(end_user), class: "text-primary" do %>
                <%= end_user.first_name %><%= end_user.last_name %>
              <% end %>
            </td>
            <td><%= end_user.email %></td>
            <% if end_user.is_vaild == false %>
              <td class= "text-success">有効</td>
            <% else %>
              <td>退会済み</td>
            <% end %>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>